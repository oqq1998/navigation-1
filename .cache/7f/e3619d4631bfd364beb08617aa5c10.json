r\n    margin: 24px 20px;\r\n}\r\n@media(min-width:500px){\r\n    .globalHeader{\r\n        margin: 80px 20px 88px;\r\n    }\r\n}\r\n.globalHeader> .searchForm{\r\n    display:flex;\r\n    justify-content: space-between;\r\n}\r\n@media(min-width:500px){\r\n    .searchForm{\r\n        max-width: 450px;\r\n        margin-left:auto; \r\n        margin-right: auto;\r\n        /* 当元素有最大宽度时可用以上方法居中 */\r\n    }\r\n}\r\n.globalHeader> .searchForm > input{\r\n    width: 100%;\r\n    margin-right: 10px;\r\n    /* 设置文本框和搜索键之间的距离为10像素 */\r\n    height:40px;\r\n    padding: 0 10px;\r\n    /* 让光标向右10像素 */\r\n    border:1px solid #ddd;\r\n    /* 覆盖原始input边框 */\r\n    border-radius: 4px;\r\n}\r\n.globalHeader> .searchForm > button{\r\n    white-space: nowrap;     \r\n    /* 如果button里是文字,这样可以禁止搜索键换行 */\r\n    padding: 0px 28px;\r\n    display:flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    /* 图标上下居中 */\r\n    border:1px solid #ddd;\r\n    border-radius: 4px;\r\n    background: #1296db;\r\n}\r\n.globalHeader> .searchForm > button .icon{\r\n    width:24px;\r\n    height:24px;\r\n}\r\n.globalContent{\r\n    max-width: 900px;\r\n    margin-left:auto; \r\n    margin-right: auto;\r\n}\r\n\r\n.siteList{\r\n    margin:32px 20px;\r\n    display: flex;\r\n    flex-wrap: wrap;\r\n    margin-right:25px ;\r\n    justify-content: space-between;\r\n}\r\n@media(min-width:500px){\r\n    .siteList{\r\n        margin-left: 0;\r\n        margin-right: -25px;\r\n        justify-content: flex-start;\r\n    }\r\n}\r\n.siteList >li{\r\n    margin-bottom: 16px;\r\n    /* 站点卡片上下隔16像素 */\r\n}\r\n@media(min-width:500px){\r\n    .siteList >li{\r\n        margin-bottom: 25px;\r\n        margin-right: 25px;\r\n    }\r\n}\r\n.siteList >li .site{\r\n    border:1px solid #ddd;\r\n    border-radius: 4px;\r\n    width:160px;\r\n    padding: 16px;\r\n    display:flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    flex-direction: column;\r\n    /* 令图标和网址居中并竖向分布 */\r\n    background: white;\r\n    position: relative;\r\n    cursor: pointer;\r\n    /* 悬浮时指针显示手形 */\r\n}\r\n.siteList >li .site> .logo{\r\n    color:#2c2c2c;\r\n    width: 56px;\r\n    height: 56px;\r\n    font-size: 50px;\r\n    display:flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    text-transform: uppercase;\r\n}\r\n.siteList >li .site> .link{\r\n    color:#2c2c2c;\r\n    font-size: 16px;\r\n    margin-top: 4px;\r\n}\r\n.siteList >li .site> .close{\r\n    position: absolute;\r\n    margin-top: 4px;\r\n    right: 10px;\r\n    top:2px;\r\n    display: none;\r\n    cursor: default;\r\n    /* 鼠标浮动到close处恢复默认指针 */\r\n}\r\n.siteList >li .site:hover > .close{\r\n    display: block;\r\n}\r\n.siteList >li .addButton{\r\n    background: white;\r\n    border:1px solid #ddd;\r\n    border-radius: 4px;\r\n    width:160px;\r\n    padding: 16px;\r\n    display:flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    flex-direction: column;\r\n}\r\n.siteList >li .addButton .icon{\r\n    width: 56px;\r\n    height: 56px;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n}\r\n.siteList >li .addButton .text{\r\n    color:#2c2c2c;\r\n    font-size: 16px;\r\n    margin-top: 4px;\r\n}"},"lineCount":null}},"error":null,"hash":"e01936feecde7915f3f0992fd46af886","cacheData":{"env":{}}}